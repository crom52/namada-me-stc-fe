    "lodash-es": "^4.17.21",
    "pinia": "^2.1.7",
    "vue": "^3.4.15",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "0.42.1",
    "@iconify/json": "^2.2.178",
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node20": "^20.1.2",
    "@types/chrome": "^0.0.260",
    "@types/node": "^20.11.10",
    "@types/webpack-env": "^1.18.3",
