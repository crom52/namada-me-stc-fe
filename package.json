    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "prettier": "^3.0.3",
    "sass": "^1.62.1",
    "typescript": "~5.3.0",
    "unocss": "^0.53.5",
    "unplugin-auto-import": "^0.16.6",
